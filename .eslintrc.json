{
    "env": {
      "browser": true,
      "es2021": true,
      "jest": true
    },
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "quotes": [2, "single", "avoid-escape"],
      "import/prefer-default-export": "off",
      "prettier/prettier": ["error", { "singleQuote": true}]
    }
}
